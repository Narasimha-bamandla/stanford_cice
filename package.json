{
	"name": "LiferayReactJsPortlet",
	"version": "1.0.0",
	"description": "LiferayReactJsPortlet",
	"devDependencies": {
		"@babel/cli": "^7.7.5",
		"@babel/core": "^7.7.5",
		"@babel/preset-env": "^7.7.6",
		"@babel/preset-react": "^7.7.4",
		"copy-webpack-plugin": "4.6.0",
		"liferay-npm-build-support": "^2.31.2",
		"liferay-npm-bundler": "^2.31.2",
		"webpack": "4.29.6",
		"webpack-cli": "3.3.0",
		"webpack-dev-server": "3.2.1"
	},
	"dependencies": {
		"@clayui/button": "^3.106.1",
		"@clayui/card": "^3.106.1",
		"@clayui/core": "^3.106.1",
		"@clayui/css": "^3.106.1",
		"@clayui/drop-down": "^3.106.1",
		"@clayui/form": "^3.106.1",
		"@clayui/layout": "^3.106.1",
		"@clayui/link": "^3.106.1",
		"@clayui/list": "^3.106.1",
		"@clayui/tabs": "^3.106.1",
		"@clayui/tooltip": "^3.106.1",
		"react": "16.8.6",
		"react-dom": "16.8.6",
		"sass": "^1.69.5"
	},
	"scripts": {
		"build": "babel --source-maps -d build src && npm run copy-assets && liferay-npm-bundler",
		"copy-assets": "lnbs-copy-assets",
		"translate": "lnbs-translate",
		"deploy": "npm run build && lnbs-deploy",
		"start": "lnbs-start"
	},
	"portlet": {
		"com.liferay.portlet.display-category": "ReactJsApp",
		"com.liferay.portlet.header-portlet-css": "/css/styles.css",
		"com.liferay.portlet.instanceable": true,
		"javax.portlet.name": "LiferayReactJsPortlet",
		"javax.portlet.security-role-ref": "power-user,user",
		"javax.portlet.resource-bundle": "content.Language"
	},
	"main": "index.js"
}
